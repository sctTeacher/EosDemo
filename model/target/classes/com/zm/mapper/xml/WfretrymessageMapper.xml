<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zm.mapper.WfretrymessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zm.model.Wfretrymessage">
        <id column="RETRYMESSAGEID" property="retrymessageid" />
        <result column="RETRYPOLICYTYPE" property="retrypolicytype" />
        <result column="RETRYLIMIT" property="retrylimit" />
        <result column="RETRYCOUNT" property="retrycount" />
        <result column="CAUSE" property="cause" />
        <result column="MESSAGE" property="message" />
        <result column="FAILDATE" property="faildate" />
        <result column="TIMEOUT" property="timeout" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        RETRYMESSAGEID AS retrymessageid, RETRYPOLICYTYPE AS retrypolicytype, RETRYLIMIT AS retrylimit, RETRYCOUNT AS retrycount, CAUSE AS cause, MESSAGE AS message, FAILDATE AS faildate, TIMEOUT AS timeout
    </sql>

</mapper>
