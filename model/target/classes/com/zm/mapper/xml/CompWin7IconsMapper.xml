<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zm.mapper.CompWin7IconsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zm.model.CompWin7Icons">
        <id column="ICON_ID" property="iconId" />
        <result column="ICON_NAME" property="iconName" />
        <result column="ICON_TEXT" property="iconText" />
        <result column="ICON_PATH" property="iconPath" />
        <result column="ICON_TITLE" property="iconTitle" />
        <result column="MENU_ID" property="menuId" />
        <result column="ICON_INDEX" property="iconIndex" />
        <result column="ICON_DESC" property="iconDesc" />
        <result column="USER_ID" property="userId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ICON_ID AS iconId, ICON_NAME AS iconName, ICON_TEXT AS iconText, ICON_PATH AS iconPath, ICON_TITLE AS iconTitle, MENU_ID AS menuId, ICON_INDEX AS iconIndex, ICON_DESC AS iconDesc, USER_ID AS userId
    </sql>

</mapper>
