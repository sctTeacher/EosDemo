<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zm.mapper.WfDimProcessactivityMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zm.model.WfDimProcessactivity">
        <id column="ACTIVITYID" property="activityid" />
        <result column="PROCESSID" property="processid" />
        <result column="PROCESSNAME" property="processname" />
        <result column="VERSIONID" property="versionid" />
        <result column="VERSIONNAME" property="versionname" />
        <result column="ACTIVITYNAME" property="activityname" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        PROCESSID AS processid, PROCESSNAME AS processname, VERSIONID AS versionid, VERSIONNAME AS versionname, ACTIVITYID AS activityid, ACTIVITYNAME AS activityname
    </sql>

</mapper>
