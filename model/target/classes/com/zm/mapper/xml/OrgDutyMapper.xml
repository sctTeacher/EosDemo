<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zm.mapper.OrgDutyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zm.model.OrgDuty">
        <id column="DUTYID" property="dutyid" />
        <result column="DUTYCODE" property="dutycode" />
        <result column="DUTYNAME" property="dutyname" />
        <result column="PARENTDUTY" property="parentduty" />
        <result column="DUTYLEVEL" property="dutylevel" />
        <result column="DUTYSEQ" property="dutyseq" />
        <result column="DUTYTYPE" property="dutytype" />
        <result column="ISLEAF" property="isleaf" />
        <result column="SUBCOUNT" property="subcount" />
        <result column="REMARK" property="remark" />
        <result column="TENANT_ID" property="tenantId" />
        <result column="APP_ID" property="appId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        DUTYID AS dutyid, DUTYCODE AS dutycode, DUTYNAME AS dutyname, PARENTDUTY AS parentduty, DUTYLEVEL AS dutylevel, DUTYSEQ AS dutyseq, DUTYTYPE AS dutytype, ISLEAF AS isleaf, SUBCOUNT AS subcount, REMARK AS remark, TENANT_ID AS tenantId, APP_ID AS appId
    </sql>

</mapper>
