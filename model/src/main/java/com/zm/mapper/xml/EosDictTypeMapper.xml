    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        DICTTYPEID AS dicttypeid, DICTTYPENAME AS dicttypename, RANK AS rank, PARENTID AS parentid, SEQNO AS seqno
    </sql>

</mapper>
