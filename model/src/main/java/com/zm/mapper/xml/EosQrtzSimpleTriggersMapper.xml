<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zm.mapper.EosQrtzSimpleTriggersMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zm.model.EosQrtzSimpleTriggers">
        <id column="TRIGGER_NAME" property="triggerName" />
        <result column="TRIGGER_GROUP" property="triggerGroup" />
        <result column="REPEAT_COUNT" property="repeatCount" />
        <result column="REPEAT_INTERVAL" property="repeatInterval" />
        <result column="TIMES_TRIGGERED" property="timesTriggered" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        TRIGGER_NAME AS triggerName, TRIGGER_GROUP AS triggerGroup, REPEAT_COUNT AS repeatCount, REPEAT_INTERVAL AS repeatInterval, TIMES_TRIGGERED AS timesTriggered
    </sql>

</mapper>
