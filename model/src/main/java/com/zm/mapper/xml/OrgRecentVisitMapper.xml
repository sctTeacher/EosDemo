<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zm.mapper.OrgRecentVisitMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zm.model.OrgRecentVisit">
        <id column="ID" property="id" />
        <result column="TARGET_ID" property="targetId" />
        <result column="USERID" property="userid" />
        <result column="FREQUENCY" property="frequency" />
        <result column="LASTTIME" property="lasttime" />
        <result column="TARGET_TYPE" property="targetType" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID AS id, TARGET_ID AS targetId, USERID AS userid, FREQUENCY AS frequency, LASTTIME AS lasttime, TARGET_TYPE AS targetType
    </sql>

</mapper>
